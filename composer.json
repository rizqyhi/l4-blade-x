    "require": {
        "php": "^7.2",
        "laravel/framework": "^4.2",
        "symfony/filesystem": "^4.0.0"
    },
    "require-dev": {
        "anlutro/l4-testing": "^0.2.9",
        "laravel/laravel": "4.2.*",
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
        "files": [
            "src/Laravel/helpers.php"
        ],
        "psr-4": {
            "Spatie\\BladeX\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\BladeX\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:keripix/laravel42x.git"
        }
    ]
